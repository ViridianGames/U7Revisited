L0AD3: db 'go home'
L0ADA: db 00
L0ADB: db 'wait here'
L0AE4: db 00
L0AE5: db 'wait here'
L0AEE: db 00
L0AEF: db '"Very well. I shall await thy return."*'
L0B16: db 00
