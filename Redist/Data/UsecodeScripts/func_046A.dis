L0081: db 'name'
L0085: db 00
L0086: db 'job'
L0089: db 00
L008A: db '"Eh, 'ow's that? Oh, m'job. Why, Oi take good care o' me bud'
 db 'dies in the graveyard 'ere in Empath Abbey, Oi do."'
L00F9: db 00
L00FA: db 'buddies'
