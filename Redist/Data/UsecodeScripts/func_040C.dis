L0000: db '"Hast thou properly searched the stables?"'
L002A: db 00
L002B: db '"What didst thou find?"'
L0042: db 00
L0043: db 'a body'
L0049: db 00
L004A: db 'a bucket'
L0052: db 00
L0053: db 'nothing'
L005A: db 00
